encoding  utf-8
language ru

source Harahora.th

select Harahora

select m1p@Harahora
select m2p@Harahora
select m3p@Harahora
select m4p@Harahora
select m5p@Harahora


export model -format survex
export model

layout normal
  #symbol-set BCRA
  #scale 1 200
  doc-author "Paweł Krawczyk"
  doc-title "Колодец в Хара-Хора"
  legend on
  statistics explo all  
  statistics topo all
  statistics carto all
  statistics copyright all
  symbol-hide group cave-centerline
  symbol-show group text
  symbol-show point cave-station
  map-comment "Баксанское ущелье, село Заюково"
endlayout

export map -format pdf -layout normal
export model -output model.kml

select m1s@Harahora
select m2s@Harahora
select m3s@Harahora
select m4s@Harahora
select m5s@Harahora

export map -projection extended -fmt pdf -layout normal -o cave_ext.pdf
